package NestedLoops;



public class Sumofprimenum {
	 public static boolean isPrime(int n) {
	        if (n <= 1) {
	            return false;  
	        }
	        for (int i = 2; i <= n / 2; i++) {
	            if (n % i == 0) {
	                return false; 
	            }
	        }
	        return true;
		}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int count = 0;
		int num = 2; 
		System.out.println("the 10 prime nu,bers are:");
		while (count < 10) {
            if (isPrime(num)) {
                System.out.println(num);
                count++;  
            }
            num++;  
        }
    }

}
